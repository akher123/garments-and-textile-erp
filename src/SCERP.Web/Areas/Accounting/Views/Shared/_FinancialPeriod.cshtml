@{
    Layout = null;
}

<tr>
    <td>@Html.DisplayName("Financial Period:")</td>
    <td>@Html.DropDownList("FpId", null, "- Select -", new { @onchange = "SetCalenderFixed(this.value)" })</td>

    <td>@Html.DisplayName("From Date:")</td>
    <td class="datepicker">@Html.TextBox("FromDate", (string)ViewBag.FromDate, new { @style = "" })</td>

    <td>@Html.DisplayName("To Date:")</td>
    <td class="datepicker">@Html.TextBox("ToDate", (string)ViewBag.ToDate, new { @style = "" })</td>
</tr>

<script src="~/Scripts/SCERP/Acc_VoucherList.js"></script>